{
  "name": "distri-lru-server",
  "version": "1.0.0",
  "engines": {
    "node": "16.x"
  },
  "description": "Redis server client for use with distri-lru library",
  "main": "src/index.ts",
  "scripts": {
    "start": "node build/src/main.js",
    "prebuild": "yarn lint",
    "build": "tsc -p tsconfig.json",
    "build:release": "rimraf && tsc -p tsconfig.release.json",
    "dev": "tsnd --respawn -r tsconfig-paths/register ./src/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "prettier": "prettier --config .prettierrc --write .",
    "prepare": "husky install"
  },
  "author": "Tenzin Pelletier <t2pellet@uwaterloo.ca>",
  "license": "AGPL-3.0-only",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "express-validator": "^6.14.2",
    "ioredis": "^5.3.2",
    "tslib": "~2.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.31",
    "@types/ioredis": "^5.0.0",
    "@types/ioredis-mock": "^8.2.5",
    "@types/jest": "~29.5",
    "@types/node": "~18",
    "@typescript-eslint/eslint-plugin": "~6.2",
    "@typescript-eslint/parser": "~6.2",
    "eslint": "~8.46",
    "eslint-config-prettier": "~9.0",
    "eslint-plugin-jest": "~27.2",
    "husky": "^8.0.0",
    "ioredis-mock": "^8.9.0",
    "jest": "~29.6",
    "prettier": "~3.0",
    "rimraf": "~5.0",
    "ts-jest": "~29.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc": "^2.0.4",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.1"
  }
}
